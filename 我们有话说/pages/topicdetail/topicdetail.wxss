/* 框架  */
.talks-layer {  
    position: fixed; 
    bottom: -100%;  
    height: 0;  
    width: 100%;  
    overflow: hidden;  
/* background-color: blue; */
}
.layer-white-space {  
    height: 100%;  
    width: 100%;  
    background-color: #ccc;  
    opacity: .5;  
/* background-color: green; */
}
.talks {  
    position: absolute;  
    height: 1100rpx;  
    width: 100%;  
    bottom: 0rpx;  
    background-color: #2f2d2e; 
    border-top-left-radius: 5%;  
    border-top-right-radius: 5%;  
    /* background-color: red; */
}
.talk-header {  
    width: 100%;  
    height: 200rpx;  
    text-align: center;
}
.talk-body {  
    height: 700rpx;
}
.talk-footer {  
    position: absolute; 
    bottom: 0rpx;  
    width: 100%;  
    height: 80rpx;  
    background-color: #151515;
}
/* 顶部元素  */
.talk-count {  
    font-size: 0.8rem;  
    height: 20rpx;  
    color: #6b696a;
}
.talk-close {  
    position: absolute;  
    top: 25rpx;  
    right: 25rpx;  
    width: 45rpx;  
    height: 45rpx;
}
/* 中部元素  */
.talk-item {  
    width: 100%;  
    background-color: #2F2D2E;
}
/* 底部元素  */
.comment_release{
  display: flex;
  align-items: flex-end; 
  justify-content: space-between; 
  box-sizing: border-box;
  /* position: fixed; */
  position: absolute; 
  left: 0;
  bottom: 0;
  width: 100%;
  padding: 8px 0 8px 13px;
  background-color: #ffffff;
  font-size: 12px;
  z-index: 999;
}
/**第一个红色的容器
 *大概想法就是给flex-direction赋一个colomn的值，并列放置
 */
.comment_release.left{
  display: flex;
  flex-direction: column;
}
/*输入框*/
.comment_release .text{
  width: 252px;
  min-height: 17px;
  max-height: 34px; /*最多显示三行*/
  line-height: 17px;
  font-size: 15px;
  margin-bottom: 4px;
  margin-left: 10px;
}
/*线*/
.comment_release .line{
margin-bottom: 0 ;
width: 252px;
height: 1rpx;
background-color: black;
}
/*发送按钮*/
.comment_release .submit{ 
  width: 80px;
  height: 27px;
  text-align: center;
  margin-right: 10px;
  background-color: #2f2d2e;
  color: #ffffff;
  font-size: 12px;
}



